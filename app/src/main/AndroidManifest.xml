<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.floydkim.lotto">

    <application
            android:allowBackup="true"
            android:fullBackupContent="false"
            android:icon="@drawable/muzi_twinkle"
            android:label="@string/app_name"
            android:roundIcon="@drawable/muzi_twinkle"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".ConstellationActivity"
                android:theme="@style/AppTheme.Constellation" />
        <activity
                android:name=".NameActivity"
                android:theme="@style/AppTheme.Name" />
        <activity
                android:name=".ResultActivity"
                android:theme="@style/AppTheme.Result" />
        <activity android:name=".AbsoluteActivity" />
        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".ConstraintLayoutActivity" />
        <!--        <activity android:name=".TestActivity" />-->
    </application>

</manifest>